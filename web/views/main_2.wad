                        <input type="text" class="form-control" id="media-type">
                    </div>
                </div>
            </div><!-- .control-group -->


            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Notification</div>
                    <div class="panel-body form-group">
                    <input type="text" class="form-control" id="order-id"   placeholder="Order ID">
                        <label><input class="radio-inline" type="radio" id="type17" name="Order_Validated">Validated</label>
                        <label><input class="radio-inline" type="radio" id="type18" name="Order_Not_Validated">Payment on Delivery (10% Deposit Received)</label>
                        <label><input class="radio-inline" type="radio" id="type19" name="Quality_Pass">Work Passed Inspection</label>
                        <label><input class="radio-inline" type="radio" id="type20" name="Quality_Failed">Work Failed Inspection</label>
                        <label><input class="radio-inline" type="radio" id="type21" name="Work_Complete">Work Complete</label>
                        <label><input class="radio-inline" type="radio" id="type22" name="Order_Complete_Deliver">Order is ready for Delivery</label>
                        <input type="text" class="form-control" id="notificationReason" placeholder="Details, if required">
                        <button class="btn btn-default" type="submit">Send Memo</button>
                    </div>

                </div>
            </div><!-- control-group -->

            <!-- <<<<<<<<<<<<<<<<<<<<<<<< Searches >>>>>>>>>>>>>>>>>>>>>>>> -->

            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Search Customers</div>
                    <div class="form-group">
                                <label for="Customer-search-type">Search for Customer records by:</label>
                                <select class="form-control" id="search-type">
                                    <option value="empID">Customer ID</option>
                                    <option value="customerLName">Last Name</option>
                                </select>
                                <h2>Search term:</h2>
                                <input type="text" class="form-control" id="Customer-search-term">
                                <button class="btn btn-default" type="submit">Search!</button>
                    </div>
                </div>
            </div><!-- control-group -->

            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Search Orders</div>
                    <div class="form-group">
                                <label for="Order-search-type">Search for Orders by:</label>
                                <select class="form-control" id="search-type">
                                    <option value="ordID">Order ID</option>
                                    <option value="customerLName">Customer Last Name</option>
                                    <option value="employeeLName">Employee Last Name</option>
                                    <option value="jobType">Job Type</option>
                                </select>
                                <h2>Search term:</h2>
                                <input type="text" class="form-control" id="Order-search-term">
                                <button class="btn btn-default" type="submit">Search!</button>
                    </div>
                </div>
            </div><!-- control-group -->

            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Search Employees</div>
                    <div class="form-group">
                                <label for="Employee-search-type">Search for Employee records by:</label>
                                <select class="form-control" id="search-type">
                                    <option value="empID">Employee ID</option>
                                    <option value="jobType">Job Title</option>
                                    <option value="employeeLName">Last Name</option>
                                </select>
                                <h2>Search term:</h2>
                                <input type="text" class="form-control" id="employee-search-term">
                                <button class="btn btn-default" type="submit">Search!</button>
                    </div>
                </div>
            </div><!-- control-group -->


            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Search Inventory</div>
                    <div class="form-group">
                                <label for="Inventory-search-type">Search Inventory by:</label>
                                <select class="form-control" id="search-type">
                                    <option value="invID">Inventory ID</option>
                                    <option value="TypID">Media Type</option>
                                </select>
                                <h2>Search term:</h2>
                                <input type="text" class="form-control" id="inventory-search-term">

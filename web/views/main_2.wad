<div class="container">
    <form role="form">

        <div class="row">
            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Admin Panel</div>
                    <div class="panel-body form-group">
                        <input type="text" class="form-control" id="employee-id"   placeholder="Employee's ID">
                        <input type="text" class="form-control" id="employee-name" placeholder="Employee's Name">
                    </div>
                </div>
            </div><!-- control-group -->


            <div class="col-sm-4 control-group">
                <div class="panel panel-default">
                    <div class="panel-heading">Create Work Order</div>
                    <div class="panel-body form-group">
                        <h2>Customer Name:</h2>
                        <input type="text" class="form-control" id="customer-name" placeholder="First Last">
                        <label><input class="radio-inline" type="radio" id="isNew" name="newExisting">New Customer </label>
                        <label><input class="radio-inline" type="radio" id="isExisting" name="newExisting">Existing Customer</label>
                        <h2>Job Type:</h2>
                        <div class="form-group">
                            <label for="job-type">Select Job Type (select one):</label>
                            <select class="form-control" id="job-type">
                                <option>Print</option>
                                <option>Engraving</option>
                            </select>
                        </div>
                        <!-- <input type="text" class="form-control" id="job-type"> -->
                        <h2>Media Type:</h2>
                        <div class="form-group">
                            <label for="media-type">Select Job Type (select one):</label>
                            <select class="form-control" id="media-type">
                                <option value="clothing">Clothing</option>
                                <option value="plague">Plague</option>
                                <option value="trophy">Trophy</option>
                            </select>
                        </div>
                        <h2>Payment Options</h2>
                        <label><input class="radio-inline" type="radio" id="inFull" name="inFull">Payed In Full </label>
                        <label><input class="radio-inline" type="radio" id="paymentOnDelivery" name="paymentOnDelivery">Payment on Delivery (10% Deposit Received)</label>
                        <label><input class="radio-inline" type="radio" id="notPaid" name="notPaid">No Payment Received </label>
                        <!-- <input type="text" class="form-control" id="media-type"> -->
                    </div>
                </div>
            </div><!-- control-group -->


